            "project": "projects/sq-ng-common/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/sq-ng-common/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/sq-ng-common/src/test.ts",
