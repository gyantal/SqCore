    "sq-ng-common": {
      "projectType": "library",
      "root": "projects/sq-ng-common",
      "sourceRoot": "projects/sq-ng-common/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
